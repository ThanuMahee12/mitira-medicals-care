# Node.js
node_modules/        # Directory for npm packages
npm-debug.log*      # Log files for npm debugging
yarn-debug.log*     # Log files for Yarn debugging
yarn-error.log      # Log files for Yarn errors
package-lock.json    # Lock file generated by npm (if not managing versions carefully)
yarn.lock            # Lock file for Yarn (if not managing versions carefully)

# Build directories
build/               # For React build output
dist/                # Distribution folder

# Environment variables
.env                 # Environment variables
.env.local           # Local environment variables
.env.development     # Development environment variables
.env.test            # Test environment variables

# Miscellaneous
coverage/            # Test coverage reports
